	public void inserer(String token) {
		explore(noeuds_base, token);
	}
	
	
	private void explore(List<Noeud> list, String token) {
		if(!isComplete(list)) {
			list.add(new Noeud(token));
			return;
		} else {
			if()
		}
	}
	
	
	public boolean isComplete(List<Noeud> ll) {
		
		for(Noeud n : ll)
		{
			if(n.equal("fin")) {
				return true;
			}
		}
		return false;
